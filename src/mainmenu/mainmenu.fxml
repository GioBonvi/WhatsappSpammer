<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="265.0" prefWidth="339.0" spacing="20" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainmenu.MainmenuController">
    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>
    <children>
        <HBox alignment="CENTER" spacing="50">
            <children>
                <Button onAction="#handleSingleMessage" text="Single message" />
                <Button onAction="#handleMessageList" text="List of messages" />
            </children>
        </HBox>
        <HBox alignment="CENTER" spacing="50">
            <children>
                <Button cancelButton="true" onAction="#handleClose" text="Close" />
            </children>
        </HBox>
    </children>
    <stylesheets>
        <URL value="@/mainmenu/mainmenu.css" />
    </stylesheets>
</VBox>
